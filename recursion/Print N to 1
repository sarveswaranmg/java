import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		printnum(n,n);    
		
	}
	static void printnum(int n,int i){
	    if(i>0){
	        System.out.print(i-- + " ");
	        printnum(n,i);
	    }
	    
	}
}

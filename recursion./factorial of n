import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		if(n<0){
		    System.out.print("Invalid");
		}
		else{
		System.out.print(factorial(n));
		}
	}
	static int factorial(int n){
	    if(n==0){
	        return 1;
	    }
	    return factorial(n-1)*n;
	}
}



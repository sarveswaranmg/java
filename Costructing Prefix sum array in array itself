public class Main
{
	public static void main(String[] args) {
	    int a[] = {1,2,3,4,5};
		prefixSum(a);
	}
	public static void prefixSum(int[] a){
	    
	    for(int i=0;i<a.length;i++){
	        if(i==0){
	            a[0] = a[i];
	        }
	        else{
	        a[i] = a[i - 1] + a[i];
	        }
	        System.out.print(a[i] + " ");
	    }
	    
	}
}

import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int result = prime(n);
		System.out.print(result);
	}
	public static int prime(int n){
	    int factors = 0;
	    for(int i = 1;i*i<=n;i++){
	        if(n%i==0){
	            factors +=2;
	        }
	    }
	    if(factors == 2){
	        return 1;
	    }
	    else{
	        return 0;
	    }
	    
	}
	
}

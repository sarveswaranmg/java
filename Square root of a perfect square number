import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int result = sqrt(n);
		System.out.print(result);
	}
	public static int sqrt(int n){
	    for(int i=1;i<=n;i++){
	        if(i*i==n){
	            return i;
	        }
	        
	    }
	    return -1;
	}
	
}
